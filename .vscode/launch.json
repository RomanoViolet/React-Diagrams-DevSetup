{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch localhost",
      "type": "firefox",
      "request": "launch",
      "reAttach": false,
      "url": "http://0.0.0.0:3000",
      "webRoot": "${workspaceFolder}",
      "pathMappings": [
        {
          "url": "webpack://projectstorm/react-diagrams-demo/src",
          "path": "${workspaceFolder}/src"
        },
        {
          "url": "webpack:///src",
          "path": "${workspaceFolder}/packages/diagrams-demo-project/src/"
        }
      ]
    }
  ]
}
