{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["google", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint", "prettier", "@emotion"],
  "rules": {
    "prettier/prettier": ["error"],
    "@emotion/jsx-import": ["error"],
    "@emotion/pkg-renaming": ["error"]
  }
}
